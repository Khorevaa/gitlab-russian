%ul.project-settings-nav.sidebar-subnav
  = nav_link(path: 'projects#edit') do
    = link_to edit_project_path(@project), title: 'Проект', class: "stat-tab tab " do
      %i.fa.fa-pencil-square-o
      %span
        Проект
  = nav_link(controller: [:project_members, :teams]) do
    = link_to namespace_project_project_members_path(@project.namespace, @project), title: 'Члены', class: "team-tab tab" do
      %i.fa.fa-users
      %span
        Члены
  = nav_link(controller: :deploy_keys) do
    = link_to namespace_project_deploy_keys_path(@project.namespace, @project), title: 'Ключи' do
      %i.fa.fa-key
      %span
        Ключи
  = nav_link(controller: :hooks) do
    = link_to namespace_project_hooks_path(@project.namespace, @project), title: 'Веб хуки' do
      %i.fa.fa-link
      %span
        Веб хуки
  = nav_link(controller: :services) do
    = link_to namespace_project_services_path(@project.namespace, @project), title: 'Службы' do
      %i.fa.fa-cogs
      %span
        Службы
  = nav_link(controller: :protected_branches) do
    = link_to namespace_project_protected_branches_path(@project.namespace, @project), title: 'Защищенные ветки' do
      %i.fa.fa-lock
      %span
        Защищенные ветки
