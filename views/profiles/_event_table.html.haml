.table-holder
  %table.table#audits
    %thead
      %tr
        %th Действие
        %th Когда

    %tbody
      - events.each do |event|
        %tr
          %td
            %span
              Signed in with 
              %b= event.details[:with]
              authentication
          %td #{time_ago_in_words event.created_at} ago
= paginate events, theme: "gitlab"
